{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "webapp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "target/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/assets/images/logo/favicon.ico",
              "src/manifest.webapp",
              "src/robots.txt",
              "src/assets"
            ],
            "styles": [
              "src/assets/scss/global.scss",
              "src/assets/scss/app.min.scss",
              "src/assets/scss/styles.scss",

              "src/assets/coza/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/coza/fonts/font-awesome-4.7.0/css/font-awesome.min.css",
              "src/assets/coza/fonts/iconic/css/material-design-iconic-font.min.css",
              "src/assets/coza/fonts/linearicons-v1.0.0/icon-font.min.css",
              "src/assets/coza/vendor/animate/animate.css",
              "src/assets/coza/vendor/css-hamburgers/hamburgers.min.css",
              "src/assets/coza/vendor/animsition/css/animsition.min.css",
              "src/assets/coza/vendor/select2/select2.min.css",
              "src/assets/coza/vendor/daterangepicker/daterangepicker.css",
              "src/assets/coza/vendor/slick/slick.css",
              "src/assets/coza/vendor/MagnificPopup/magnific-popup.css",
              "src/assets/coza/vendor/perfect-scrollbar/perfect-scrollbar.css",
              "src/assets/coza/css/util.css",
              "src/assets/coza/css/main.css",

              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/ngx-toastr/toastr.css",

              "src/assets/scss/force-system-fonts.scss"
            ],

            "scripts": [
              "src/assets/coza/vendor/jquery/jquery-3.2.1.min.js",
              "src/assets/coza/vendor/animsition/js/animsition.min.js",
              "src/assets/coza/vendor/bootstrap/js/popper.js",
              "src/assets/coza/vendor/bootstrap/js/bootstrap.min.js",
              "src/assets/coza/vendor/select2/select2.min.js",
              "src/assets/coza/vendor/daterangepicker/moment.min.js",
              "src/assets/coza/vendor/daterangepicker/daterangepicker.js",
              "src/assets/coza/vendor/slick/slick.min.js",
              "src/assets/coza/js/slick-custom.js",
              "src/assets/coza/vendor/parallax100/parallax100.js",
              "src/assets/coza/vendor/MagnificPopup/jquery.magnific-popup.min.js",
              "src/assets/coza/vendor/isotope/isotope.pkgd.min.js",
              "src/assets/coza/vendor/sweetalert/sweetalert.min.js",
              "src/assets/coza/vendor/perfect-scrollbar/perfect-scrollbar.min.js",
              "src/assets/coza/js/main.js",
              "src/assets/coza/js/coza-init.js",

              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]

          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "3MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "50kB",
                  "maximumError": "100kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "open": true
          },
          "configurations": {
            "production": {
              "buildTarget": "webapp:build:production"
            },
            "development": {
              "buildTarget": "webapp:build:development",
              "hmr": true
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "styles": [
              "src/assets/scss/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "cache": {
      "enabled": true,
      "path": "./target/angular/",
      "environment": "all"
    },
    "packageManager": "npm",
    "analytics": "7fbd9657-7178-4c41-8739-35474f2aaa35"
  }
}
